<p id="notice"><%= notice %></p>

<h1>Listing Skillers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Social link 1</th>
      <th>Social link 2</th>
      <th>Location</th>
      <th>Bio</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skillers.each do |skiller| %>
      <tr>
        <td><%= skiller.first_name %></td>
        <td><%= skiller.last_name %></td>
        <td><%= skiller.social_link_1 %></td>
        <td><%= skiller.social_link_2 %></td>
        <td><%= skiller.location %></td>
        <td><%= skiller.bio %></td>
        <td><%= skiller.user %></td>
        <td><%= link_to 'Show', skiller %></td>
        <td><%= link_to 'Edit', edit_skiller_path(skiller) %></td>
        <td><%= link_to 'Destroy', skiller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skiller', new_skiller_path %>
